<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('lib/semantic/semantic.min.css') }}">
    {% endblock %}
</head>
<body>
<div class="ui stackable menu">
    <div class="item">
        <img src="/images/logo.png" alt="Tiflo">
    </div>
    <a class="item">Home</a>
    <a class="item">About me</a>
    <a class="item">Services</a>
    <div class="right menu">
        <div class="item">
            <div id="Signup" data-url="{{ path('app_register') }}" class="ui green button">Sign Up</div>
        </div>
        <div class="item">
            <div id="Signin" data-url="{{ path('app_login') }}" class="ui primary button">Sign In</div>
        </div>
    </div>
</div>
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset('lib/jquery-3.3.1.min.js') }}"></script>
    <script src="{{ asset('lib/semantic/semantic.min.js') }}"></script>
    <script src="{{ asset('js/base/base.js') }}"></script>
{% endblock %}
</body>
</html>
